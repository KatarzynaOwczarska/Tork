        	reset: true,
        	delay: 500,
        	distance: 0
    	});
    	sr.reveal('#fioletowy-napis', {
        	reset: true,
        	duration: 1000,
        	delay: 500,
        	easing: 'linear',
        	distance: 0
    	});
