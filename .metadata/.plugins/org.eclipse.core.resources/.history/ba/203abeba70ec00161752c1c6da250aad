	<script>
		$(document).ready(function(){
			$('*[data-animate]').addClass('ukryj').each(function(){
      			$(this).viewportChecker({
        			classToAdd: 'show animated ' + $(this).data('animate'),
        			classToRemove: 'ukryj',
        			offset: '5%'
      			});
    		});
		});
		
		function check() {
			var scrollPos = controller.info("scrollPos");
			document.getElementById("test").innerHTML = scrollPos;
		}
	</script>
